<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.up899244.DontLookForWaterResult"
    android:background="@drawable/cave">

    <TextView
        android:id="@+id/text1"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="You stayed in the cave but you got a fever and you didn't get better. Game over"
        android:textColor="#000000"
        android:layout_marginTop="84dp"
        android:textSize="20sp"
        android:background="#9FFFFFFF"
        android:layout_width="425dp"
        android:layout_height="115dp"/>

    <Button
        android:id="@+id/button"
        android:layout_width="193dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginTop="48dp"
        android:background="#9FFFFFFF"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="Start from the beginning"
        android:textSize="20sp"
        android:textStyle="bold" />


</RelativeLayout>